{
  "name": "builddotfolio",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "redux-starter-kit": "^0.5.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-links": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "babel-loader": "^8.0.6",
    "eslint-plugin-jasmine": "^2.10.1",
    "jasmine-core": "^3.4.0",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-junit-reporter": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^4.0.2",
    "node-sass": "^4.12.0",
    "stylelint": "^10.1.0",
    "stylelint-a11y": "^1.2.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^3.8.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "yarn eslint && yarn stylelint",
    "lint-fix": "yarn eslint-fix && yarn stylelint-fix",
    "stylelint": "stylelint 'src/**/*.scss'",
    "stylelint-fix": "yarn stylelint --fix",
    "eslint": "eslint 'src/**/*.js'",
    "eslint-fix": "yarn eslint --fix",
    "test": "karma start",
    "test-debug": "karma start --log-level debug --jasmine-timeout=300000 --browsers=Firefox",
    "test-ci": "karma start --progress=false --singleRun=true --reporters=spec,junit --browsers=Chrome,Firefox",
    "validate": "yarn lint-fix && yarn test-ci",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
